{"ast":null,"code":"export default {\n  name: \"EditView\",\n  props: ['id'],\n  data() {\n    return {\n      loading: false,\n      post: null,\n      postResult: null\n    };\n  },\n  created() {\n    this.fetchData();\n  },\n  watch: {\n    '$route': 'fetchData'\n  },\n  methods: {\n    fetchData() {\n      this.post = null;\n      this.loading = true;\n      fetch('https://localhost:7007/expenses/').then(r => r.json()).then(json => {\n        this.post = json;\n        this.loading = false;\n        return;\n      });\n    },\n    fortmatResponse(res) {\n      return JSON.stringify(res, null, 2);\n    },\n    postData() {\n      const postData = {\n        name: this.$refs.post_name.value,\n        amount: this.$refs.post_amount.value,\n        transactionDate: this.$refs.post_transactionDate.value\n      };\n      fetch(`https://localhost:7007/expenses/`, {\n        method: \"post\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify(postData)\n      });\n    },\n    clearPostOutput() {\n      this.postResult = null;\n    }\n  }\n};","map":{"version":3,"names":["name","props","data","loading","post","postResult","created","fetchData","watch","methods","fetch","then","r","json","fortmatResponse","res","JSON","stringify","postData","$refs","post_name","value","amount","post_amount","transactionDate","post_transactionDate","method","headers","body","clearPostOutput"],"sources":["C:\\Users\\bajna\\Desktop\\temp\\koltsegvetes-koveto\\vueapp\\src\\views\\Expenses\\EditView.vue"],"sourcesContent":["<template>\r\n    <div id=\"app\" class=\"container\">\r\n        <div class=\"card\" v-if=\"post\">\r\n            <div class=\"card-header\">Vue Fetch POST</div>\r\n            <div class=\"card-body\" v-for=\"expense in post\" :key=\"expense.id\">\r\n                <div class=\"form-group\">\r\n                    <input type=\"text\" class=\"form-control\" ref=\"post_id\" :value=expense.id disabled />\r\n   