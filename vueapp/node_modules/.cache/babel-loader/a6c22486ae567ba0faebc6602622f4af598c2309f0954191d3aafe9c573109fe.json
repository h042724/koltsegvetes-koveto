\r\n                const postData = {\r\n                    name: this.$refs.post_name.value,\r\n                    amount: this.$refs.post_amount.value,\r\n                    transactionDate: this.$refs.post_transactionDate.value,\r\n                };\r\n\r\n                fetch(`https://localhost:7007/expenses/`, {\r\n                    method: \"post\",\r\n                    headers: {\r\n                        \"Content-Type\": \"application/json\"\r\n                    },\r\n                    body: JSON.stringify(postData),\r\n                })\r\n            },\r\n\r\n            clearPostOutput() {\r\n                this.postResult = null;\r\n            },\r\n        }\r\n    }\r\n</script>"],"mappings":"AA2BI,eAAe;EACXA,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE,CAAC,IAAI,CAAC;EACbC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,KAAK;MACdC,IAAI,EAAE,IAAI;MACVC,UAAU,EAAE;IAChB;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,SAAS,EAAE;EACpB,CAAC;EACDC,KAAK,EAAE;IACH,QAAQ,EAAE;EACd,CAAC;EACDC,OAAO,EAAE;IACLF,SAASA,CAAA,EAAG;MACR,IAAI,CAACH,IAAG,GAAI,IAAI;MAChB,IAAI,CAACD,OAAM,GAAI,IAAI;MAEnBO,KAAK,CAAC,kCAAkC,EACnCC,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACC,IAAI,EAAE,EAClBF,IAAI,CAACE,IAAG,IAAK;QACV,IAAI,CAACT,IAAG,GAAIS,IAAI;QAChB,IAAI,CAACV,OAAM,GAAI,KAAK;QACpB;MACJ,CAAC,CAAC;IACV,CAAC;IACDW,eAAeA,CAACC,GAAG,EAAE;MACjB,OAAOC,IAAI,CAACC,SAAS,CAACF,GAAG,EAAE,IAAI,EAAE,CAAC,CAAC;IACvC,CAAC;IAEDG,QAAQA,CAAA,EAAG;MACP,MAAMA,QAAO,GAAI;QACblB,IAAI,EAAE,IAAI,CAACmB,KAAK,CAACC,SAAS,CAACC,KAAK;QAChCC,MAAM,EAAE,IAAI,CAACH,KAAK,CAACI,WAAW,CAACF,KAAK;QACpCG,eAAe,EAAE,IAAI,CAACL,KAAK,CAACM,oBAAoB,CAACJ;MACrD,CAAC;MAEDX,KAAK,CAAE,kCAAiC,EAAE;QACtCgB,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UACL,cAAc,EAAE;QACpB,CAAC;QACDC,IAAI,EAAEZ,IAAI,CAACC,SAAS,CAACC,QAAQ;MACjC,CAAC;IACL,CAAC;IAEDW,eAAeA,CAAA,EAAG;MACd,IAAI,CAACxB,UAAS,GAAI,IAAI;IAC1B;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}