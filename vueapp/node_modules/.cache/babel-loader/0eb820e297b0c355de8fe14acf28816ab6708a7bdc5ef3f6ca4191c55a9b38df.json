ss=\"col-md-4\" v-for=\"expense in post\" :key=\"expense.id\">\r\n                        <div class=\"text-danger\"></div>\r\n                        <input type=\"number\" name=\"Id\" :value=expense.id v-model=\"id\" disabled />\r\n                        <div class=\"form-group\">\r\n                            <label for=\"Name\" class=\"control-label\">Expense Name</label>\r\n                            <input name=\"Name\" class=\"form-control\" :value=expense.name v-model=\"name\" />\r\n                        </div>\r\n                        <div class=\"form-group\">\r\n                            <label for=\"Amount\" class=\"control-label\">Amount</label>\r\n                            <input name=\"Amount\" class=\"form-control\" :value=expense.amount v-model=\"amount\" />\r\n                        </div>\r\n                        <div class=\"form-group\">\r\n                            <label for=\"TransactionDate\" class=\"control-label\">Transaction Date</label>\r\n                            <input name=\"TransactionDate\" class=\"form-control\" :value=expense.transactionDate v-model=\"transactionDate\" />\r\n                        </div>\r\n                        <div class=\"form-group\">\r\n                            <input type=\"submit\" value=\"Save\" class=\"btn btn-primary\" v-on:click=\"editExpense()\" />\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </form>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'IndexView',\r\n    components: {},\r\n    props: ['id'],\r\n    data() {\r\n        return {\r\n            loading: false,\r\n            post: null,\r\n            name: \"\",\r\n            amount: 0,\r\n            transactionDate: \"\"\r\n        };\r\n    },\r\n    created() {\r\n        // fetch the data when the view is created and the data is\r\n        // already being observed\r\n        this.fetchData();\r\n    },\r\n    watch: {\r\n        // call again the method if the route changes\r\n        '$route': 'fetchData'\r\n    },\r\n    methods: {\r\n        fetchData() {\r\n            this.post = null;\r\n            this.loading = true;\r\n\r\n            fetch('https://localhost:7007/expenses/')\r\n                .then(r => r.json())\r\n                .then(json => {\r\n                    this.post = json;\r\n                    this.loading = false;\r\n                    return\r\n                });\r\n        },\r\n        editExpense() {\r\n            const requestOptions = {\r\n                method: 'POST',\r\n                headers: {\r\n                    'Content-Type': 'application/json',\r\n                    'Authorization': 'Bearer my-token',\r\n                    'My-Custom-Header': 'foobar'\r\n                },\r\n                body: JSON.stringify({ title: 'Vue POST Request Example' })\r\n            };\r\n            fetch('https://localhost:7007/expenses', requestOptions)\r\n                .then(response => response.json())\r\n                .then(json => {\r\n                    this.name = json.name;\r\n                    this.amount = json.amount;\r\n                    this.transactionDate = json.transactionDate;\r\n                    return\r\n                });\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAgCA,eAAe;EACXA,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE,CAAC,CAAC;EACdC,KAAK,EAAE,CAAC,IAAI,CAAC;EACbC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,KAAK;MACdC,IAAI,EAAE,IAAI;MACVL,IAAI,EAAE,EAAE;MACRM,MAAM,EAAE,CAAC;MACTC,eAAe,EAAE;IACrB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN;IACA;IACA,IAAI,CAACC,SAAS,EAAE;EACpB,CAAC;EACDC,KAAK,EAAE;IACH;IACA,QAAQ,EAAE;EACd,CAAC;EACDC,OAAO,EAAE;IACLF,SAASA,CAAA,EAAG;MACR,IAAI,CAACJ,IAAG,GAAI,IAAI;MAChB,IAAI,CAACD,OAAM,GAAI,IAAI;MAEnBQ,KAAK,CAAC,kCAAkC,EACnCC,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACC,IAAI,EAAE,EAClBF,IAAI,CAACE,IAAG,IAAK;QACV,IAAI,CAACV,IAAG,GAAIU,IAAI;QAChB,